class Solution:
    def twoSum(self, nums: List[int], target: int) -> List[int]:

        for itr_1, ele_1 in enumerate(nums):
            for itr_2, ele_2 in enumerate(nums):
                if itr_1 != itr_2 and ele_1 + ele_2 == target:
                    return [itr_1, itr_2]                      
